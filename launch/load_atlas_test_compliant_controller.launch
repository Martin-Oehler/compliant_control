<?xml version="1.0"?>
<launch>
  
    <!--Provide option to set namespace from outside-->
    <arg name="namespace" default="joint_controllers"/>

    <rosparam file="$(find vigir_compliant_ros_controller)/config/atlas_test.yaml" command="load" ns="$(arg namespace)"/>
    <node name="compliant_controller_spawner" pkg="controller_manager" type="spawner" ns="$(arg namespace)" respawn="false" output="screen" args="left_arm_compliant_controller" />
    
</launch> 
